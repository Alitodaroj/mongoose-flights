<%- include('../partials/header') %>

<form id="new-form" action="/flights" method="POST">
  <label>Airline:</label>
  <select name="airline">
    <option value="American">American</option>
    <option value="Jetblue">Jetblue</option>
    <option value="United">United</option>
</select>

  <label>Airport Year:</label>
    <select name="airport">
      <option value="LAX">LAX</option>
      <option value="JFK">JFK</option>
      <option value="AUS">AUS</option>
      <option value="DEN">DEN</option>
    </select>
    <label>Flight Number</label>
  <input type="number" name="flightNo">
  <label>Departs:</label>
  <input type="date" name="departs">
  <input type="submit" name="submit">
</form>

<form id="add-performer-form" action="/ticket" method="POST">
    <label>Name:</label>
    <input type="text" name="name">
    <label>Born:</label>
    <input type="date" name="born">
    <input type="submit" value="Add Performer">
  </form>
  

<%- include('../partials/footer') %>
